<!DOCTYPE html>
<html>
<head>
  <title>@ViewBag.Title</title>
  <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
  <link href="@Url.Content("~/Content/CourseMain.css")" rel="stylesheet" type="text/css" />
  <link href="@Url.Content("~/Content/jquery-ui-1.8.16.custom.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/jquery-1.6.2.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery-ui-1.8.16.custom.min.js")" type="text/javascript"></script>

</head>
<body>
  <div class="page">
    <div id="header">
      <div id="title">
        <h1>
          Course manager</h1>
      </div>
      <div id="menucontainer">
        <ul id="menu">
          <li>@Html.ActionLink(CourseManager.UI.Resources.Home_Link_Text, "Index", "Home")</li>
          <li>@Html.ActionLink(CourseManager.UI.Resources.Teacher_Entity_Collection_Label, "Index", "Teacher")</li>
          <li>@Html.ActionLink(CourseManager.UI.Resources.Semester_LinkName, "Index", "Semester")</li>
          <li>@Html.ActionLink(CourseManager.UI.Resources.Group_Link_Text, "Index", "Group")</li>
        </ul>
      </div>
    </div>
    <div id="main">
      @RenderBody()
      <div id="footer">
      </div>
    </div>
  </div>
</body>
</html>
